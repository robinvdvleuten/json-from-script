{
  "name": "json-from-script",
  "version": "1.0.0",
  "description": "Parse any JSON objects from your CSP aware script tags.",
  "license": "MIT",
  "author": "Robin van der Vleuten <robin@webstronauts.co>",
  "main": "dist/json-from-script.js",
  "module": "dist/json-from-script.esm.js",
  "keywords": [
    "json",
    "script",
    "csp",
    "security"
  ],
  "files": [
    "src",
    "dist/*.js"
  ],
  "scripts": {
    "build": "node ./build",
    "flow": "flow",
    "test": "jest --env=jsdom",
    "format": "prettier --single-quote --trailing-comma es5 --write '{src,test,build}/**/*.js'",
    "precommit": "lint-staged --verbose",
    "prepublish": "yarn build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinvdvleuten/json-from-script.git"
  },
  "bugs": {
    "url": "https://github.com/robinvdvleuten/json-from-script/issues"
  },
  "homepage": "https://github.com/robinvdvleuten/json-from-script#readme",
  "devDependencies": {
    "babel-jest": "^20.0.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.46.0",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "lint-staged": "^3.4.1",
    "p-map": "^1.1.1",
    "prettier": "^1.3.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "uglify-js": "^3.0.3"
  }
}
