{
  "name": "json-from-script",
  "version": "1.3.1",
  "description": "Tiny 236b JSON parser for your CSP aware script tags",
  "license": "MIT",
  "author": "Robin van der Vleuten <robin@webstronauts.co>",
  "main": "dist/json-from-script.js",
  "browser": "dist/json-from-script.umd.js",
  "jsnext:main": "src/index.js",
  "keywords": [
    "json",
    "script",
    "csp",
    "security"
  ],
  "files": [
    "src",
    "dist/*.js"
  ],
  "scripts": {
    "build": "mkdir -p dist && npm run -s build:cjs && npm run -s build:umd",
    "build:cjs": "rollup -c -f cjs | uglifyjs -cm -o $npm_package_main",
    "build:umd": "rollup -c -f umd | uglifyjs -cm -o $npm_package_browser",
    "format": "prettier --single-quote --trailing-comma es5 --write '{src,test}/**/*.js'",
    "precommit": "lint-staged --verbose",
    "prepublish": "yarn build",
    "test": "jest --env=jsdom"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robinvdvleuten/json-from-script.git"
  },
  "bugs": {
    "url": "https://github.com/robinvdvleuten/json-from-script/issues"
  },
  "homepage": "https://github.com/robinvdvleuten/json-from-script#readme",
  "devDependencies": {
    "babel-jest": "^20.0.1",
    "babel-preset-env": "^1.4.0",
    "husky": "^0.13.3",
    "jest": "^20.0.1",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.16.0",
    "uglify-js": "^3.0.3"
  }
}
